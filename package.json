{
  "name": "@nuka9510/simple-enum",
  "version": "1.1.0",
  "description": "simple enum util class",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.min.mjs",
      "require": "./dist/cjs/index.min.cjs"
    },
    "./js-util": {
      "types": "./dist/js-util.d.ts",
      "import": "./dist/esm/js-util.min.mjs",
      "require": "./dist/cjs/js-util.min.cjs"
    },
    "./@types/enum": {
      "types": "./@types/enum.d.ts"
    },
    "./@types/js-util/util": {
      "types": "./@types/js-util/util.d.ts"
    }
  },
  "files": [
    "@types",
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -r -f dist && tsc && webpack --config webpack.config.cjs",
    "postversion": "git push && git push --tag && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuka9510/simple-enum.git"
  },
  "keywords": [
    "util",
    "enum",
    "module",
    "CommonJS",
    "ES Module",
    "cjs",
    "esm"
  ],
  "author": "nuka9510",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuka9510/simple-enum/issues"
  },
  "homepage": "https://github.com/nuka9510/simple-enum",
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/webpack": "^5.28.5",
    "ts-node": "^10.9.2",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@nuka9510/js-util": "^1.2.4"
  }
}
